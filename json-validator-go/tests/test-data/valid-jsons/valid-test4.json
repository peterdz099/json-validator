{
    "PolicyName": "DynamoDBReadWriteAccessWithSid",
    "PolicyDocument": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Sid": "DynamoDBReadWriteSid",
          "Effect": "Allow",
          "Action": [
            "dynamodb:GetItem",
            "dynamodb:PutItem",
            "dynamodb:DeleteItem"
          ],
          "Resource": "arn:aws:dynamodb:us-east-1:123456789012:table/ExampleTable"
        }
      ]
    }
  }